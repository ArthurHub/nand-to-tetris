/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a, b=b, out=tmp1);
    Not(in=a,out=na);
    Not(in=b,out=nb);
    Nand(a=na, b=nb, out=tmp2);
    And(a=tmp1, b=tmp2, out=out);
}